.lab-item-types.next{data: {role: "temporary-other-container"}}
  = render "fixed_lab_item_types_title_bar", title: "Select a type", back: "types"
  - Labs::Catalog.new.items["Temporary/Other"]["type"].each do |type|
    = button_tag type, class: "btn btn-primary type-button", data: {next: "[data-role='occlusion-container']"}, role: "item-option"
.lab-item-types.next{data: {role: "occlusion-container"}}
  = render "fixed_lab_item_types_title_bar", title: "Select occlusion type", back: "temporary-other"
  - Labs::Catalog.new.items.lab_item["occlusion"].each do |occlusion|
    = button_tag occlusion, class: "btn btn-primary type-button", data: {next: "[data-role='contacts-container']"}, role: "item-option"
.lab-item-types.next{data: {role: "contacts-container"}}
  = render "fixed_lab_item_types_title_bar", title: "Select contact type", back: "occlusion"
  - Labs::Catalog.new.items.lab_item["contacts"].each do |contact|
    = button_tag contact, class: "btn btn-primary type-button", data: {role: "finisher"}, role: "item-option"
